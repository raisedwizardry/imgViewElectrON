{
    "name": "imgViewElectrON",
    "version": "0.3.0",
    "description": "imgViewElectrON",
    "keywords": [
        "image",
        "picture"
    ],
    "author": "raisedwizardry",
    "license": "MIT",
    "repository": "http://www.github.com/raisedwizardry/imgviewelectron",
    "main": "src/js/main.js",
    "scripts": {
        "start": "electron .",
        "startone": "electron . --inspect=5858 src/img/sample1.jpg",
        "startanother": "electron . src/img/sample2.jpg",
        "starttwo": "electron . src/img/sample1.jpg src/img/sample2.jpg",
        "pack": "electron-builder --dir",
        "dist": "electron-builder"
    },
    "build": {
        "appId": "com.imgViewElectrON"
    },
    "dependencies": {
        "electron-window-manager": "^1.0.6",
        "image-size": "^0.7.4"
    },
    "devDependencies": {
        "cypress": "^3.4.1",
        "electron": "^6.0.7",
        "electron-builder": "^21.2.0"
    }
}
